# Database Configuration
POSTGRES_USER=n8n
POSTGRES_PASSWORD=n8n_secure_password
POSTGRES_DB=n8n
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# PgBouncer Configuration
PGBOUNCER_PORT=6432
PGBOUNCER_POOL_MODE=transaction
PGBOUNCER_MAX_CLIENT_CONN=100
PGBOUNCER_DEFAULT_POOL_SIZE=25

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=redis_secure_password

# n8n Configuration
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin_secure_password
N8N_HOST=192.168.5.12
N8N_PORT=5678
N8N_PROTOCOL=http
WEBHOOK_URL=http://192.168.5.12:5678/

# n8n Database Configuration (via PgBouncer)
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=pgbouncer
DB_POSTGRESDB_PORT=6432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=n8n_secure_password

# n8n Queue Mode Configuration
EXECUTIONS_MODE=queue
QUEUE_BULL_REDIS_HOST=redis
QUEUE_BULL_REDIS_PORT=6379
QUEUE_BULL_REDIS_PASSWORD=redis_secure_password

# n8n Worker Configuration
QUEUE_HEALTH_CHECK_ACTIVE=true
QUEUE_RECOVERY_INTERVAL=30

# n8n Encryption
N8N_ENCRYPTION_KEY=your_encryption_key_here_32_chars

# Timezone
GENERIC_TIMEZONE=Asia/Jakarta
TZ=Asia/Jakarta
N8N_SECURE_COOKIE=false
N8N_EDITOR_BASE_URL=http://192.168.5.12:5678
N8N_DISABLE_UI=false
N8N_SKIP_WEBHOOK_DEREGISTRATION_SHUTDOWN=true
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=false
